{
    "name": "amirax/base62",
    "description": "Base62 encoder and decoder",
    "keywords": ["base62", "encode", "decode"],
    "homepage": "https://github.com/amirax/base62",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Max Voronov",
            "email": "maxivoronov@gmail.com",
            "homepage": "https://github.com/MaxVoronov",
            "role": "Developer"
        }
    ],
    "support": {
        "source": "https://github.com/amirax/base62",
        "issues": "https://github.com/amirax/base62/issues"
    },
    "require": {
        "php": "^5.4 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Amirax\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Amirax\\": "tests"
        }
    },
    "scripts": {
        "test": "composer phpcs && composer phpunit",
        "phpcs": "vendor/bin/phpcs --standard=PSR2 --extensions=php --encoding=utf-8 --ignore=*/vendor/* . -p",
        "phpunit": "vendor/bin/phpunit"
    }
}
